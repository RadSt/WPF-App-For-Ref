				<!-- Ширина двери. -->
				<width>2000</width>
				<!-- Высота двери. -->
				<height>2300</height>
				<!-- Смещение двери относительно центра вагона. -->
				<center_distance>2000</center_distance>
			</door>
			<!-- Высота скругленного свода (крыши) вагона в центре. -->
			<roof>200</roof>
