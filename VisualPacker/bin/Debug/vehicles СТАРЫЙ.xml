<?xml version="1.0" encoding="windows-1251"?>
<!-- Данный файл на примере описывает формат XML -файла для линейки программ packer3d-ver3, который содержит список транспортых средств. -->
<!-- Файл содержит список ТС, предназначенных для укладки в них указанного набора ящиков. -->

<!-- Элементы файла содержат атбрибут val, по которому на этот элемент можно сослаться из другого места файла -->
<!-- Когда файл сохранятеся программой, атрибут val автоматически присваивается всем элементам, однако при ручном формировании файла -->
<!-- этот атрибут можно определить уникальным образом только для тех элементов, на которые будут ссылки из другой части файла. -->

<!-- Обязательный корневой элемент dapl, с атрибутами (обязательно должны присутствовать): -->
<!-- - object="vehicle" -->
<!-- - version="1.3" -->
<dapl object="vehicle" version="1.3">
	<!-- Обязательный объединяющий элемент vehicles: -->
  <vehicles val="1">
		<!-- Каждое ТС описывается тегом vehicle, который содержит перечисление характеристик ТС. -->
		<!-- Обязательным атрибутом тега vehicle должен быть kind, который опеределяет вид ТС: -->
	  <!-- - container - контейнер -->
	  <!-- - lorry - 2-х осный грузовик или прицеп -->
	  <!-- - freight_car - вагон -->
	  <!-- Разные виды ТС содержат разный набор параметров -->
	  



<!-- Пример описания вагона -->
    <vehicle val="2" kind="freight_car">
	<!-- Название ТС - не более 1000 симовлов -->
      <name>Вагон 120</name>
	 <!-- Длина -->
      <length>15720</length>
      <!-- Ширина -->
      <width>2770</width>
      <!-- Высота -->
      <height>2800</height>
	<!-- Грузоподъемность ТС (если не ограниченно, значение не указывается) -->
      <tonnage>50000</tonnage>
<!-- Положение двери на боковой стенке вагона (вагон грузится от двери к краям). -->
      <door>
<!-- Ширина двери. -->
        <width>2000</width>
<!-- Высота двери. -->
 <height>2300</height>
		<!-- Смещение двери относительно центра вагона. -->
 <center_distance>2000</center_distance>
 </door>
<!-- Высота скругленного свода (крыши) вагона в центре. -->
 <roof>200</roof>
 </vehicle>

