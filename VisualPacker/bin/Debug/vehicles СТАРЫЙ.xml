<!-- Расстояние от оси, до начала грузового отсека (часто отрицатльное, когда ось находится за пределами грузового отсека, под кабиной) -->
   <front_distance>1000</front_distance>
<!-- Максимальная доспустимая нагрузка на ось. -->
        <max_tonnage>6000</max_tonnage>
	<!-- Нагрузка на ос при пустом транспортном средстве. -->
        <empty_tonnage>0</empty_tonnage>
      </axle_front>
<!-- Параметры задней оси -->
 <axle_back>
<!-- Расстояние от оси, до конца грузового отсека -->
   <back_distance>1000</back_distance>
<!-- Максимальная доспустимая нагрузка на ось. -->
