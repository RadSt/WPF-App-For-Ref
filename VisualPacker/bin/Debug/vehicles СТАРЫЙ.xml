      <!-- Длина -->
      <length>13600</length>
      <!-- Ширина -->
      <width>2450</width>
      <!-- Высота -->
      <height>2450</height>
			<!-- Грузоподъемность ТС (если не ограниченно, значение не указывается) -->
      <tonnage>22000</tonnage>
			<!-- Параметры передней оси -->
